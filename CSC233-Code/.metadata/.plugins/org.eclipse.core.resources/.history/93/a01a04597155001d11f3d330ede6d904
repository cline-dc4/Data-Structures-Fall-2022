
public class Driver 
{

	public static void main(String[] args) 
	{
		SortedLinkedList ll = new SortedLinkedList();
		
		try
		{
		ll.addItem("University");
		ll.addItem("Corban");
		ll.addItem("Hello");
		ll.addItem("Data");
		ll.addItem("University");
		}
		catch(Exception e)
		{
			System.out.println(e);
		}
		System.out.println(ll);
		
		try
		{
			ll.deleteItem("Hello");
		}
		catch(Exception e)
		{
			System.out.println(e);
		}
		System.out.println(ll);
	}

}
